{
  "name": "myaza-software/cycle-database-bundle",
  "type": "symfony-bundle",
  "description": "Bundle cycle/database for symfony",
  "license": "MIT",
  "authors": [
    {
      "name": "Vlad Shashkov",
      "email": "root@myaza.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cycle\\Bundle\\Database\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Bundle\\Database\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "cycle/database": "^1.0",
    "doctrine/sql-formatter": "^1.1",
    "myaza-software/bridge-spiral-core": "^0.1",
    "symfony/config": "^4.3.3 || ^5.0",
    "symfony/console": "^4.3.3 || ^5.0",
    "symfony/dependency-injection": "^4.3.3 || ^5.0",
    "symfony/framework-bundle": "^4.3.3 || ^5.0",
    "symfony/http-kernel": "^4.3.3 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.5",
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpstan/phpstan": "^0.12.83",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpstan/extension-installer": "^1.1",
    "infection/infection": ">=0.15.3",
    "symfony/web-profiler-bundle": "^4.3.3|^5.0",
    "symfony/yaml": "^4.3.3 || ^5.0",
    "twig/twig": "^2.12|^3.0"
  },
  "config": {
    "sort-packages": true
  }
}
